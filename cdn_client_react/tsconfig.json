{
  "compilerOptions": {
    "baseUrl": ".",
    // "paths": {
    //   "@components/*": [
    //     "src/components/*"
    //   ],
    //   "@contexts/*": [
    //     "src/contexts/*"
    //   ],
    //   "@content/*": [
    //     "src/content/*"
    //   ],
    //   "@layouts/*": [
    //     "src/layouts/*"
    //   ],
    //   "@models/*": [
    //     "src/models/*"
    //   ],
    //   "@stores/*": [
    //     "src/stores/*"
    //   ],
    //   "@theme/*": [
    //     "src/theme/*"
    //   ],
    // },
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "*.ts",
    "*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}